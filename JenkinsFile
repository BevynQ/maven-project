pipeline {
    agent any

    stages {
        stage("Build"){

            steps {
                echo 'building'
                bat 'mvn clean package'
            }
            post {
                success {
                    echo 'Now Archiving...'
                    archiveArtifacts: '**/target/*.war'
                }
            }

        }
        satge ('Deploy to Staging'){
            steps {
                build job: 'Deply-to-staging'
            }
        }
    }
}